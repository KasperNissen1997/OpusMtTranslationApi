    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Current File with CMD arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "--models", 
                // opus-mt-da-de,opus-mt-de-da,opus-mt-da-fi,opus-mt-da-es,opus-mt-es-da,opus-mt-da-fr,opus-mt-da-ru,opus-mt-ru-da
                "opus-mt-da-en,opus-mt-en-da,opus-mt-da-no,opus-mt-no-da", 
                "--overwrite_mar",
            ]
        }
    ]
}